package main

import &amp;#34;fmt&amp;#34;

func fibonacci(c, quit chan int) {
 x, y := 0, 1
 for {
 select {
 case c &amp;lt;- x:
 x, y = y, x&amp;#43;y
 case &amp;lt;-quit:
 fmt.Println(&amp;#34;quit&amp;#34;)
 return
 }
 }
}

func main() {
 c := make(chan int)
 quit := make(chan int)
 go func() {
 for i := 0; i &amp;lt; 10; i&amp;#43;&amp;#43; {
 fmt.Println(&amp;lt;-c)
 }
 quit &amp;lt;- 0
 }()
 fibonacci(c, quit)
}

